{
  "name": "crawler",
  "version": "1.0.0",
  "description": "WebdriverJS POM Example",
  "main": "./src/test.ts",
  "scripts": {
    "scrape": "npx tsx src/scrape.ts",
    "test": "mocha --require @babel/register -r ts-node/register --timeout 9000000 src/test.ts",
    "test:report": "mocha --require @babel/register -r ts-node/register  --reporter mochawesome --reporter-options autoOpen=true,reportDir=report,reportFilename=comics-search src/test.ts",
    "build": "npx babel ./src --out-dir lib --extensions '.ts'",
    "tsc": "tsc",
    "eslint": "eslint . ",
    "eslint:fix": "eslint .  --fix",
    "format": "prettier --write './**/**/**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx,html}'",
    "format:check": "prettier --check './**/**/**/*.{js,mjs,cjs,ts,mts,cts,jsx,tsx,html}'"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@babel/core": "^7.28.0",
    "@babel/register": "^7.27.1",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.2.0",
    "@types/selenium-webdriver": "^4.1.28",
    "ts-node": "^10.9.2",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-tailwindcss": "3.18.2",
    "eslint-plugin-zod": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "pretty-quick": "^4.2.2",
    "chai": "^5.2.1",
    "mocha": "^11.7.1",
    "mochawesome": "^7.1.3",
    "screenshot-desktop": "^1.15.1",
    "selenium-webdriver": "^4.34.0",
    "typescript": "^5.9.2"
  }
}